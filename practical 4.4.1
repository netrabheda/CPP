#include<iostream>
using namespace std;

int max(int m, int n) {
    return (m > n) ? m : n;
}

int max(int m, int n, int o) {
    return max(max(m, n), o);
}

int main() {

    int num1, num2, num3;

      cout << "Enter three numbers: ";
      cin >> num1 >> num2 >> num3;

      cout << "The maximum number is: " << max(num1, num2, num3) << endl;
      
    return 0;
}
