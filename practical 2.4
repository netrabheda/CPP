#include<iostream>
using namespace std;

int main()
{
    int a[100],n,p,q,temp;
    cout<<"Enter The Limit : ";
    cin>>n;

    cout<<"Enter "<<n<<" Value :"<<endl;
    
    for(p=0;p<n;p++)
    {
        cin>>a[p];
    }

    for(p=0;p<n;p++)
    {
        for(q=p+1;q<n;q++)
        {
            if(a[p]>a[q])
            {
                temp=a[p];
               a[p]=a[q];
               a[q]=temp;
            }
        }
    }

    cout<<"Sorted Array : "<<endl;
    for(p=0;p<n;p++)
    {
        cout<<a[p]<<endl;
    }
    return 0;
}
